{
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "env": {
      "browser": true
    },
    "parser": "babel-eslint",
    "rules": {
      "react/jsx-filename-extension": 0,
      "react/prefer-es6-class": 0,
      "react/prop-types": 0,
      "no-plusplus": 0,
      "no-alert": 0,
      "react/jsx-no-bind": 0,
      "arrow-parens": [
        "error",
        "always"
      ]
    }
}
