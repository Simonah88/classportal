    "tslint": "tslint --project .",
    "lint": "npm run eslint; npm run tslint"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "rules": {
      "import/no-extraneous-dependencies": [
        2,
        {
          "devDependencies": true
        }
      ],
      "react/jsx-filename-extension": 0
    }
  }
