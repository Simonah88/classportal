  "scripts": {
    "clean": "rm -rf ./typings/; rm -rf ./node_modules/;",
    "configure": "npm install; typings install --global;",
    "build": "tsc",
    "test": "mocha -c",
    "cover": "istanbul cover _mocha",
    "start:dev": "webpack-dev-server --inline --content-base frontend/public --history-api-fallback",
